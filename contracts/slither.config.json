{
  "filter_paths": [
    "lib/",
    "test/",
    "script/",
    "out/"
  ],
  "exclude_dependencies": true,
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "exclude_optimization": false,
  "solc_remaps": [
    "@chainlink/contracts/=lib/chainlink-brownie-contracts/contracts/src/v0.8/",
    "@openzeppelin/contracts/=lib/openzeppelin-contracts/contracts/"
  ],
  "detectors_to_exclude": [
    "pragma",
    "solc-version",
    "low-level-calls",
    "naming-convention"
  ],
  "detectors_to_include": [
    "reentrancy-eth",
    "reentrancy-no-eth",
    "reentrancy-unlimited-gas",
    "uninitialized-state",
    "uninitialized-storage",
    "arbitrary-send-eth",
    "controlled-delegatecall",
    "delegatecall-loop",
    "msg-value-loop",
    "tx-origin",
    "suicidal",
    "assembly",
    "assert-state-change",
    "boolean-equal",
    "dangerous-unary",
    "return-bomb",
    "incorrect-shift",
    "multiple-constructors",
    "name-reused",
    "protected-vars",
    "public-mappings-nested",
    "rtlo",
    "shadowing-state",
    "uninitialized-local",
    "unused-return",
    "incorrect-equality",
    "boolean-cst",
    "similar-names",
    "encode-packed-collision",
    "arbitrary-send-erc20",
    "domain-separator-collision",
    "enum-conversion",
    "erc20-interface",
    "erc721-interface",
    "incorrect-unary",
    "locked-ether",
    "mapping-deletion",
    "shadowing-abstract",
    "tautology",
    "write-after-write",
    "reentrancy-benign",
    "reentrancy-events",
    "timestamp",
    "assembly-usage",
    "deprecated-standards",
    "erc20-indexed",
    "function-init-state",
    "low-level-calls",
    "missing-zero-check",
    "reentrancy-unlimited-gas",
    "too-many-digits",
    "cache-array-length",
    "constable-states",
    "external-function",
    "immutable-states",
    "var-read-using-this"
  ],
  "printers_to_run": [
    "human-summary",
    "inheritance-graph",
    "contract-summary"
  ]
}
